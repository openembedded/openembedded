rootfs               /                    auto       defaults              1 1
proc                 /proc                proc       defaults              0 0
devpts               /dev/pts             devpts     mode=0620,gid=5       0 0
usbfs                /proc/bus/usb        usbfs      auto                0 0
tmpfs                /dev/shm             tmpfs      mode=0777             0  0
tmpfs                /var/volatile        tmpfs      mode=0755             0 0
