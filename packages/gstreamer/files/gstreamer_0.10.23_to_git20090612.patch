+  for (i = 0; i < iters; i++) {
+    add_remove_test4 ();
+  }
+  g_print ("add/destroy/remove test4 %d in thread %ld\n", iters,
+      vmsize () - usage1);
+
+  g_print ("leaked: %ld\n", vmsize () - usage1);
+
+  return (vmsize () - usage1 ? -1 : 0);
+}
