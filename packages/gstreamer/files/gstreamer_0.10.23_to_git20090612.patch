+  if (state == GST_STATE_PAUSED && running)
+    done = TRUE;
+}
+
+int
+main (gint argc, gchar * argv[])
+{
+  int runs = MAX_IDENTITIES * RUNS_PER_IDENTITY;
+  int i;
+  gulong id;
+  GstElement *thread;
+
+  gst_init (&argc, &argv);
+
+  for (i = 0; i < runs; ++i) {
