+  ASSERT_CRITICAL (gst_buffer_list_iterator_take (it, buf));
+  ASSERT_BUFFER_REFCOUNT (buf, "buf", 2);
+  fail_unless (gst_buffer_list_iterator_next_group (it));
+  ASSERT_CRITICAL (gst_buffer_list_iterator_take (it, buf));
