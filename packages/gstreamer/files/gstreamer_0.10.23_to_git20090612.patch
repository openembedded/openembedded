+
+  caps = gst_caps_intersect (gst_static_caps_get (&rawcaps3),
+      gst_static_caps_get (&rawcaps5));
+  parent =
+      xmlNewChild (doc->xmlRootNode, NULL, (const xmlChar *) "Capabilities4",
